        {{!-- <p id="randA">{{a}}</p> --}}
        <input type="text" name="num1" value="{{a}}">
    </div>
    <div class="divC divB">
        {{!-- <p id="randB">{{b}}</p> --}}
        <input type="text" name="num2" value="{{b}}">
    </div>
    <div class="btnDiv">
        <button id="randMethod">
            <a href="/random">Rand Number</a>
        </button>
        <button id="btnAddList">
            <a href="/addList">Add to Linklist</a>
        </button>
        <button id="btnAddQueue">
            <a href="/addQueue">Add to Queue</a>
        </button>
